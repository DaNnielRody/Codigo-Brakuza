programa {
  funcao inicio() {
    real numero1, numero2, numero3

    escreva("Digite o primeiro número: ")
    leia(numero1)
    escreva("Digite o segundo número: ")
    leia(numero2)
    escreva("Digite o terceiro número: ")
    leia(numero3)

    escreva("A média é: ", ((numero1 + numero2 + numero3) / 3))
  }
}
